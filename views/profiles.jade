table.table.table-striped.table-bordered.table-hover.contacts.users
  tbody
    each user in users
      tr
        td.text.name= user.name
        td.text.email= user.email
        td.text.address= user.address
        td.text.zipcode= user.zipcode
        td.text.city= user.city
        td.text.state= user.state
